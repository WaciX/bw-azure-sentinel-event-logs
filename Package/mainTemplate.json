                                    "Custom-Bitwarden_ClientCredentials": {
                                        "columns": [
                                            {
                                                "name": "date",
                                                "type": "string",
                                                "description": "The date/timestamp when the event occurred"
                                            },
                                            {
                                                "name": "type",
                                                "type": "int",
                                                "description": "Type of the event"
                                            },
                                            {
                                                "name": "itemId",
                                                "type": "string",
                                                "description": "The unique identifier of the event"
                                            },
                                            {
                                                "name": "collectionId",
                                                "type": "string",
                                                "description": "Collection Id"
                                            },
                                            {
                                                "name": "groupId",
                                                "type": "string",
                                                "description": "Group Id"
                                            },
                                            {
                                                "name": "policyId",
                                                "type": "string",
                                                "description": "Policy Id"
                                            },
                                            {
                                                "name": "memberId",
                                                "type": "string",
                                                "description": "Member Id"
                                            },
                                            {
                                                "name": "actingUserId",
                                                "type": "string",
                                                "description": "Acting User Id"
                                            },
                                            {
                                                "name": "installationId",
                                                "type": "string",
                                                "description": "Installation Id"
                                            },
                                            {
                                                "name": "device",
                                                "type": "int",
                                                "description": "Device Id"
                                            },
                                            {
                                                "name": "ipAddress",
                                                "type": "string",
                                                "description": "The IP address of the acting user"
                                            }
                                        ]
                                    }
                                },
                                "destinations": {
                                    "logAnalytics": [
                                        {
                                            "workspaceResourceId": "[variables('workspaceResourceId')]",
                                            "name": "clv2ws1"
                                        }
                                    ]
                                },
                                "dataFlows": [
                                    {
                                        "streams": [
                                            "Custom-Bitwarden_ClientCredentials"
                                        ],
                                        "destinations": [
                                            "clv2ws1"
                                        ],
                                        "transformKql": "source | extend DeviceNamesJson = parse_json(\"{\\\"0\\\":\\\"Android\\\",\\\"1\\\":\\\"iOS\\\",\\\"2\\\":\\\"Chrome Extension\\\",\\\"3\\\":\\\"Firefox Extension\\\",\\\"4\\\":\\\"Opera Extension\\\",\\\"5\\\":\\\"Edge Extension\\\",\\\"6\\\":\\\"Windows\\\",\\\"7\\\":\\\"macOS\\\",\\\"8\\\":\\\"Linux\\\",\\\"9\\\":\\\"Chrome\\\",\\\"10\\\":\\\"Firefox\\\",\\\"11\\\":\\\"Opera\\\",\\\"12\\\":\\\"Edge\\\",\\\"13\\\":\\\"Internet Explorer\\\",\\\"14\\\":\\\"Unknown Browser\\\",\\\"15\\\":\\\"Android\\\",\\\"16\\\":\\\"UWP\\\",\\\"17\\\":\\\"Safari\\\",\\\"18\\\":\\\"Vivaldi\\\",\\\"19\\\":\\\"Vivaldi Extension\\\",\\\"20\\\":\\\"Safari Extension\\\",\\\"21\\\":\\\"SDK\\\",\\\"22\\\":\\\"Server\\\",\\\"23\\\":\\\"Windows CLI\\\",\\\"24\\\":\\\"MacOs CLI\\\",\\\"25\\\":\\\"Linux CLI\\\"}\") | extend deviceName = tostring(DeviceNamesJson[tostring(device)]) | project TimeGenerated = todatetime([\"date\"]), EventType = type, itemId, collectionId, groupId, policyId, memberId, actingUserId, installationId, device, deviceName, ipAddress",
                                        "outputStream": "Custom-BitwardenEventLogs_CL"
                                    }
                                ]
                            }
                        },
                        {
                            "name": "BitwardenEventLogs_CL",
                            "apiVersion": "2022-10-01",
                            "type": "Microsoft.OperationalInsights/workspaces/tables",
                            "location": "[parameters('workspace-location')]",
                            "kind": null,
                            "properties": {
                                "schema": {
                                    "name": "BitwardenEventLogs_CL",
                                    "columns": [
                                        {
                                            "name": "TimeGenerated",
                                            "type": "Datetime",
