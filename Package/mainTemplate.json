                                    "email": "support@bitwarden.com",
                                    "tier": "Partner",
                                    "link": "https://www.bitwarden.com"
                                }
                            }
                        },
                        {
                            "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', 'BitwardenEventLogs')]",
                            "apiVersion": "2023-02-01-preview",
                            "type": "Microsoft.OperationalInsights/workspaces/providers/dataConnectors",
                            "location": "[parameters('workspace-location')]",
                            "kind": "RestApiPoller",
                            "properties": {
                                "connectorDefinitionName": "BitwardenEventLogs",
                                "dataType": "BitwardenEventLogs_CL",
                                "dcrConfig": {
                                    "streamName": "Custom-Bitwarden_ClientCredentials",
                                    "dataCollectionEndpoint": "[[parameters('dcrConfig').dataCollectionEndpoint]",
                                    "dataCollectionRuleImmutableId": "[[parameters('dcrConfig').dataCollectionRuleImmutableId]"
                                },
                                "auth": {
                                    "type": "OAuth2",
                                    "ClientSecret": "[[parameters('ClientSecret')]",
                                    "ClientId": "[[parameters('ClientId')]",
                                    "GrantType": "client_credentials",
                                    "TokenEndpoint": "https://identity.bitwarden.com/connect/token",
                                    "TokenEndpointHeaders": {
                                        "Content-Type": "application/x-www-form-urlencoded"
                                    },
                                    "scope": "api.organization"
                                },
                                "request": {
                                    "apiEndpoint": "https://api.bitwarden.com/public/events",
                                    "httpMethod": "GET",
                                    "queryWindowInMin": 5,
                                    "queryTimeFormat": "yyyy-MM-ddTHH:mm:ss.000000Z",
                                    "startTimeAttributeName": "start",
                                    "endTimeAttributeName": "end",
                                    "rateLimitQps": 1,
                                    "retryCount": 3,
                                    "timeoutInSeconds": 30,
                                    "headers": {
                                        "Accept": "application/json"
                                    }
                                },
                                "response": {
                                    "eventsJsonPaths": [
                                        "$.data"
                                    ],
                                    "format": "json"
                                },
                                "paging": {
                                    "pagingType": "NextPageToken",
                                    "nextPageParaName": "continuationToken",
                                    "nextPageTokenJsonPath": "$.continuationToken"
                                }
                            }
                        }
                    ]
                },
