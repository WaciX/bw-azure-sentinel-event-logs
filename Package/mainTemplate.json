                                },
                                "author": {
                                    "name": "zieniuk.maciej@gmail.com"
                                },
                                "support": {
                                    "name": "Bitwarden.com",
                                    "email": "support@bitwarden.com",
                                    "tier": "Partner",
                                    "link": "https://www.bitwarden.com"
                                },
